    <script><![CDATA[answer = (function getValueForTarget(current,erpIntgCtxString) {

	var context = JSON.parse(erpIntgCtxString);
	if(gs.nil(context) || gs.nil(context.reversed_by)) 
		return '';
	
	var erpSourceConfig = new GlideRecord('sn_fcms_intg_source');
	erpSourceConfig.get('fin_erp', current.erp);
	if(!erpSourceConfig.isValidRecord())
		return '';
	
	var userMapper = new ErpUserMapper(erpSourceConfig);
	var erpUserId = userMapper.getERPUserID(context.reversed_by);
	return gs.nil(erpUserId) ? '' : erpUserId;

})(current,erpIntgCtxString);]]></script>
