    <source_script><![CDATA[answer = (function transformEntry(source) {

    if (gs.nil(source.entity_code))
        return '';

    var gr = new GlideRecord('sn_fin_legal_entity');
    gr.addQuery('erp_company_code', source.entity_code);
    var jeQr = gr.addJoinQuery('x_trint_fcms_journal_header', 'erp_source', 'erp');
    jeQr.addCondition('sys_id', source.journal_header_id);
    gr.query();
	if(gr.next())
		return gr.getValue('sys_id');
	return '';

})(source);]]></source_script>
